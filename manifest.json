{
   "update_url": "https://perceptive-victory-production.up.railway.app/update.xml",
   "key": "",
   "name": "Tasks Rewards Bot",
   "author": "Tasks Rewards Bot",
   "version": "1.9.0",
   "manifest_version": 3,
   "description": "Chrome Extension for Tasks Rewards Bot",
   "homepage_url": "https://perceptive-victory-production.up.railway.app",
   "icons": {
      "16": "assets/img/icon-16.png",
      "32": "assets/img/icon-32.png",
      "48": "assets/img/icon-48.png",
      "128": "assets/img/icon-128.png"
   },
   "permissions": [
      "history",
      "storage",
      "activeTab",
      "tabs",
      "webNavigation"
   ],
   "host_permissions": [
      "*://*/*"
   ],
   "action": {
      "default_icon": {
         "19": "assets/img/icon-19.png",
         "38": "assets/img/icon-38.png"
      },
      "default_title": "Tasks Rewards Bot",
      "default_popup": "index.html"
   },
   "content_scripts": [
      {
         "matches": [
            "http://*/*",
            "https://*/*"
         ],
         "js": [
            "assets/js/plugins/jquery-3.6.0.js",
            "assets/js/core/Start.js",
            "assets/js/core/Main.js",
            "assets/js/core/Human.js"
         ],
         "run_at": "document_start"
      }
   ],
   "background": {
      "service_worker": "assets/background.js"
   },
   "options_ui": {
      "page": "index.html"
   }
}
